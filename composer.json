{
    "name": "voila_cms/crudbooster",
    "description": "CRUDBooster Is CRUD Generator Based On Laravel Easy To Make An Advanced Backend",
    "type": "library",
    "license": "MIT",
    "keywords":[
        "laravel",
        "admin-panel",
        "backend",
        "crud-generator",
        "crudbooster",
        "crud",
        "generator"
    ],
    "authors": [
        {
            "name": "Ferry",
            "email": "ferry@crocodic.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php":"^7.2",
        "barryvdh/laravel-dompdf": "0.8.*",
        "doctrine/inflector": "^1.4",
        "doctrine/dbal": "^2.10",
        "nesbot/carbon": "^2.31",
        "doctrine/common": "^2.13",
        "carbonphp/carbon-doctrine-types": "^1.0 <2.0",
        "maatwebsite/excel": "^3.1",
        "intervention/imagecache":"^2.3",
        "laravel/framework": "^6.18|^7.28|^8.34",
        "intervention/image": "^2.7",
        "tecnickcom/tcpdf": "^6.6"
    },
    "autoload": {
        "psr-4": {
          "crocodicstudio\\crudbooster\\": "src/"
        }
      },
    "extra": {
        "laravel": {
            "providers": [
                "crocodicstudio\\crudbooster\\CRUDBoosterServiceProvider"
            ]
        }
    }
}
